            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    const images = document.querySelectorAll('.l-main__MARKit__left img');
                    let currentIndex = 0;
                    const imageCount = images.length;
                    const intervalTime = 3000; // 3秒ごとに切り替え

                    // 画像の切り替えを行う関数
                    function switchImage() {
                        // 現在表示されている画像を非表示にする
                        images[currentIndex].classList.remove('active');

                        // 次の画像に切り替え
                        currentIndex = (currentIndex + 1) % imageCount;

                        // 次の画像を表示
                        images[currentIndex].classList.add('active');
                    }

                    // 初期表示：最初の画像に active クラスを付与
                    images[currentIndex].classList.add('active');

                    // 一定時間ごとに画像を切り替える
                    setInterval(switchImage, intervalTime);
                });


                document.addEventListener('DOMContentLoaded', function () {
                    const items = document.querySelectorAll('.l-main__MARKit__productImage');
                    const itemCount = items.length;
                    let currentIndex = 0;

                    // 初期化：全てのアイテムを非表示
                    function hideAllItems() {
                        items.forEach(item => item.classList.remove('visible'));
                    }

                    // アイテムを順次表示させる
                    function showNextItem() {
                        // 現在のインデックスまでのアイテムを表示
                        for (let i = 0; i <= currentIndex; i++) {
                            items[i].classList.add('visible');
                        }

                        // 次のアイテムに進む
                        currentIndex++;

                        // 最後のアイテムまで表示されたら最初に戻る
                        if (currentIndex >= itemCount) {
                            setTimeout(() => {
                                hideAllItems(); // 全てのアイテムを非表示に戻す
                                currentIndex = 0; // インデックスをリセット
                            }, 2000); // 全て表示後、2秒待ってリセット
                        }
                    }

                    // 2秒ごとにアイテムを順次表示
                    setInterval(showNextItem, 1000);
                });</script>

